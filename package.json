{
    "name": "parking-system",
    "version": "1.0.0",
    "description": "Full-stack Parking Management System",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"green,blue\"",
        "dev:backend": "php artisan serve --host=127.0.0.1 --port=8000",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:integrated": "npm run build:assets && npm run dev:backend",
        "install:all": "composer install && cd frontend && npm install && cd ..",
        "install:backend": "composer install",
        "install:frontend": "cd frontend && npm install",
        "build:frontend": "cd frontend && npm run build",
        "build:assets": "npm run build && npm run dev",
        "test:backend": "php artisan test",
        "test:frontend": "cd frontend && npm run test",
        "migrate": "php artisan migrate",
        "migrate:fresh": "php artisan migrate:fresh --seed",
        "seed": "php artisan db:seed"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.1.0",
        "vite": "^6.0"
    },
    "dependencies": {
        "@vitejs/plugin-react": "^4.7.0",
        "js-cookie": "^3.0.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^7.8.1"
    }
}
